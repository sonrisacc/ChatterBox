{
  "presets": [
    "react",
    "es2015",
    "stage-2",
    "stage-0",
    [
      "env",
      {
        "targets": {
          "browsers": "last 2 versions"
        },
        "loose": true,
        "modules": false
      }
    ]
  ],
  "env": {
    "server": {
      "plugins": ["transform-es2015-modules-commonjs"]
    },
    "development": {
      "plugins": ["transform-es2015-modules-commonjs"]
    }
  },
  "plugins": [
    "react-hot-loader/babel",
    "babel-plugin-transform-class-properties"
  ]
}
